    "label" : "$select",
    "description" : "only select $select properties of an Entity/-Set",
    "optional" : true,
    "constraints" : {
      "notEmpty" : false,
      "pattern" : {
        "value" : "^\\S*$",
        "message" : "must not contain any whitespace!"
