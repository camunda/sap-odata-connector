    "binding" : {
      "name" : "httpMethod.httpMethod",
      "type" : "zeebe:input"
    },
    "type" : "Dropdown",
    "choices" : [ {
      "name" : "GET",
      "value" : "get"
    }, {
      "name" : "POST",
      "value" : "post"
    }, {
      "name" : "PUT",
      "value" : "put"
    }, {
