    "constraints" : {
      "notEmpty" : true
    },
    "feel" : "optional",
    "group" : "sap",
    "binding" : {
      "name" : "destination",
      "type" : "zeebe:input"
    },
    "type" : "String"
  }, {
    "id" : "oDataService",
