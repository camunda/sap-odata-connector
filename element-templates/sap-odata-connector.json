    "value" : "V2",
    "group" : "sap",
    "binding" : {
      "name" : "httpMethod.oDataVersionPatch",
      "type" : "zeebe:input"
    },
    "condition" : {
      "property" : "httpMethod.httpMethod",
      "equals" : "patch",
      "type" : "simple"
    },
    "type" : "Dropdown",
    "choices" : [ {
      "name" : "V2",
      "value" : "V2"
    }, {
      "name" : "V4",
      "value" : "V4"
    } ]
  }, {
    "id" : "httpMethod.oDataVersionDelete",
