    "type" : "Dropdown",
    "choices" : [ {
      "name" : "V2",
      "value" : "V2"
    }, {
      "name" : "V4",
      "value" : "V4"
    } ]
  }, {
    "id" : "httpMethod.oDataVersionPatch",
    "label" : "OData version",
    "description" : "what version of the OData protocol the above service uses",
