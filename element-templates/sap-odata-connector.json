    "optional" : false,
    "constraints" : {
      "notEmpty" : true,
      "pattern" : {
        "value" : "^[^/].*$",
        "message" : "entityOrEntitySet must not start with a '/'"
      }
    },
    "feel" : "optional",
    "group" : "sap",
    "binding" : {
      "name" : "entityOrEntitySet",
      "type" : "zeebe:input"
