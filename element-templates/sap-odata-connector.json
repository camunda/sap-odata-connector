    "feel" : "optional",
    "group" : "advanced",
    "binding" : {
      "name" : "httpMethod.select",
      "type" : "zeebe:input"
    },
    "condition" : {
      "property" : "httpMethod.httpMethod",
      "equals" : "get",
      "type" : "simple"
    },
    "type" : "String"
