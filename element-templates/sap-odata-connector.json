{
  "$schema" : "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name" : "SAP OData Connector",
  "id" : "io.camunda.connector.sap.odata.outbound.v1",
  "documentationRef" : "https://docs.camunda.io/xxx",
  "version" : 1,
  "category" : {
    "id" : "connectors",
